{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/backup-to-sheets",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/decade-judgment",
      "schedule": "0 0 11,21 * *"
    },
    {
      "path": "/api/cron/check-escalation",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/daily-summary",
      "schedule": "0 23 * * *"
    }
  ]
}